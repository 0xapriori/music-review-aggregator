{
  "name": "music-review-aggregator",
  "version": "1.0.0",
  "description": "A website aggregating music reviews from Anthony Fantano and Pierre Scaruffi",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "better-sqlite3": "^9.6.0",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "natural": "^6.12.0",
    "node-cron": "^3.0.3",
    "p-limit": "^4.0.0",
    "stopword": "^2.0.8",
    "winston": "^3.11.0",
    "youtube-transcript": "^1.0.6"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.1"
  },
  "keywords": [
    "music",
    "reviews",
    "fantano",
    "scaruffi",
    "aggregator"
  ],
  "author": "Music Review Aggregator",
  "license": "MIT"
}

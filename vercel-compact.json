{
  "version": 2,
  "name": "music-review-aggregator-compact",
  "builds": [
    {
      "src": "server-compact.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-compact.js"
    },
    {
      "src": "/(.*)",
      "dest": "/server-compact.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "server-compact.js": {
      "maxDuration": 10
    }
  }
}